<Window x:Class="FileAndFolderDialog.Wpf.Samples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileAndFolderDialog.Wpf.Samples"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="SelectFileButton" Width="150" Margin="10" Content="Select File Dialog Sample" Click="SelectFileButton_Click"/>
        <Button x:Name="SaveFileButton" Width="150" Margin="10" Content="Save File Dialog Sample" Click="SaveFileButton_Click"/>
    </StackPanel>
</Window>
